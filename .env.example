# UL Transit Data Collector Environment Variables
# Copy this file to .env and fill in your values

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost/ul_transit_data
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_password
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=ul_transit_data

# API Keys
UL_API_KEY=your_ul_api_key_here

# Application Settings
DATA_COLLECTION_INTERVAL=5  # in seconds
EVENT_PROCESSING_INTERVAL=10  # in seconds
WEBSOCKET_HEARTBEAT_INTERVAL=30  # in seconds

# Redis (optional - for enhanced event management)
REDIS_URL=redis://localhost:6379/0

# Debug Mode (set to "true" to enable mock data generation)
DEBUG_MODE=false 